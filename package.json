{
  "name": "floer-soloer",
  "version": "1.0.0",
  "description": "Template for FloerSoloer, by FloerSoloer",
  "license": "ISC",
  "author": "FloerSoloer",
  "repository": "https://github.com/FloerSoloer/FloerSoloer",
  "private": true,
  "main": "dist/bundle.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "prettier --write --loglevel silent . && eslint --fix .",
    "prepare": "husky install",
    "serve": "yarn build && yarn start",
    "start": "cross-env NODE_PATH=dist node dist/index.js",
    "start:dev": "ts-node -P server/tsconfig.json -r tsconfig-paths/register server/index.ts",
    "test": "jest",
    "build": "yarn build:client && yarn build:server",
    "build:server": "tsc -p server",
    "build:client": "webpack --mode production",
    "commit": "cz",
    "dev": "nodemon",
    "dev:server": "nodemon --config server/nodemon.json",
    "dev:client": "nodemon --config client/nodemon.json"
  },
  "dependencies": {
    "aws-sdk": "^2.1199.0",
    "aws-secrets-manager-cache": "^0.2.0",
    "bcryptjs": "^2.4.3",
    "dotenv-safe": "^8.2.0",
    "dynamoose": "^3.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.49",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.8.1",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "node": "^16.13.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "styles-loader": "^4.0.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.6.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yarn": "^1.22.19"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.(css|html|json)": [
      "prettier --write"
    ],
    "*.(ts|tsx)": [
      "eslint --fix"
    ]
  }
}
